.jumbotron
  .container
    %h1 gCamp
    %p Your life organized
.container
  .row
  - @col.each do |col|
    .col-md-4
      .smallimg
        = image_tag (col[:img])
      .panel.panel-info
        .panel-heading
          %h2.panel-title= col[:title]
        .panel-body
          - col[:item].each do |list|
            %li= list
.container
  - quotes = [['"You da bomb"', "-Atom"],['"This site looks killer"', "-Orca"],['"What a dope page"',"-Snoop"]]
    %ul
    - quotes.each do |quote|
    %li.quotes= quote[0]
    %li.quotes.text-muted= quote[1]
    %br
